<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android"
    android:ordering="sequentially">

    <set android:ordering="together" android:duration="150" android:interpolator="@android:anim/accelerate_interpolator">

        <objectAnimator
            android:propertyName="translationY"
            android:valueFrom="0"
            android:valueTo="20"
            android:valueType="floatType" />

    </set>

    <set android:ordering="together" android:duration="150" android:interpolator="@android:anim/decelerate_interpolator">

        <objectAnimator
            android:propertyName="translationY"
            android:valueFrom="20"
            android:valueTo="0"
            android:valueType="floatType" />



    </set>


</set>